{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "module": "ESNext",
        "skipLibCheck": true,
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "baseUrl": "..",
        "typeRoots": [
            "Setup/node_modules/@types"
        ],
        "paths": {
            "@/*": [
                "../apps/web/src/*"
            ],
            "@atlas/shared/*": [
                "../../packages/shared/src/*"
            ],
            "react": [
                "node_modules/react"
            ],
            "react-dom": [
                "node_modules/react-dom"
            ],
            "react-router-dom": [
                "node_modules/react-router-dom"
            ],
            "react-router": [
                "node_modules/react-router"
            ],
            "firebase/*": [
                "node_modules/firebase/*"
            ],
            "react/*": [
                "node_modules/react/*"
            ],
            "react/jsx-runtime": [
                "node_modules/react/jsx-runtime"
            ],
            "react-dom/*": [
                "node_modules/react-dom/*"
            ]
        }
    },
    "include": [
        "../../apps/web/src/**/*.ts",
        "../../apps/web/src/**/*.tsx",
        "../../packages/shared/src/**/*.ts",
        "../../apps/web/src/vite-env.d.ts"
    ]
}